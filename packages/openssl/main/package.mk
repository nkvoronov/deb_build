# Имя пакета
PKG_NAME="openssl"

# Версия пакета
PKG_VERSION="1.1.1f"

# Версия пакета префикс
PKG_VERSION_PRE="none"

# Версия пакета ревизия
PKG_VERSION_REV="none"

# Тип получаемых исходников (git,svn,hg,web)
PKG_SOURCE_TYPE="web"

# Путь к исходникам
PKG_SOURCE_DEST="https://launchpad.net/ubuntu/+archive/primary/+sourcefiles/openssl/1.1.1f-1ubuntu2.13/openssl_1.1.1f.orig.tar.gz"

# Тип версии исходников (none,branch,tags,revision)
PKG_SOURCE_TREE_VERSION_TYPE="none"

# Название для типа версии (если пусто то ("none"))
PKG_SOURCE_TREE_VERSION_NAME="none"

# Имя папки в архиве (только для web но не пустой ("none"))
PKG_SOURCE_DIR="openssl-1.1.1f"

# Вариант сборки
PKG_PPA="ppa1"

# Имя ресурса на launchpad
PKG_LAUNCHPAD_REPOSITORY="jellyfin"

# Очистка CVS (.git, .svn, .hg)
PKG_CVS_CLEAR="no"
